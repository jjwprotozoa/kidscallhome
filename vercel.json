{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.kidscallhome.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "kidscallhome.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/og-image.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    },
    {
      "source": "/icon-*.png",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/png"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

